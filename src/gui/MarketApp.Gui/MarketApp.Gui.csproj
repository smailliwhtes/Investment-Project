<Project Sdk="Microsoft.NET.Sdk.Maui">
  <PropertyGroup>
    <TargetFramework>net8.0-windows10.0.19041.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <RootNamespace>MarketApp.Gui</RootNamespace>
    <UseMaui>true</UseMaui>
    <SingleProject>true</SingleProject>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <EnableDefaultMauiItems>true</EnableDefaultMauiItems>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CommunityToolkit.Maui" Version="9.1.1" />
    <PackageReference Include="LiveChartsCore.SkiaSharpView.Maui" Version="2.0.0-rc6.1" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\MarketApp.Gui.Core\MarketApp.Gui.Core.csproj" />
  </ItemGroup>
</Project>
